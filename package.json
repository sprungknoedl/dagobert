{
  "name": "dagobert",
  "version": "4.0-deku-tailwind",
  "private": "true",
  "scripts": {
    "build-purs": "spago build",
    "build-css": "postcss ./css/index.css -o ./dist/dagobert.css",
    "watch-css": "postcss -w ./css/index.css -o ./dist/dagobert.css",
    "bundle": "spago bundle --minify && postcss css/index.css -o dist/dagobert.css",
    "serve": "go build && ./dagobert"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "esbuild": "^0.19.9",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "purescript": "^0.15.13",
    "purs-tidy": "^0.10.0",
    "spago": "^0.93.18",
    "tailwindcss": "^3.3.5",
    "vanilla-framework": "^4.5.0"
  },
  "dependencies": {
    "yaml": "^2.3.4"
  }
}
